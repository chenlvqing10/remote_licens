/ {

pinctrl: pinctrl@04015000 {
	compatible = "lombo,n7-pinctrl";
	reg = <0x04015000 0x1000>;
	clocks = <&apb1_gate 3>, <&apb1_reset 3>;
	clock-names = "apb_gpio_gate", "apb_gpio_reset";

	/*
	 * Pin banks
	 */

	gpa: gpa {
		gpio-controller;
		#gpio-cells = <2>;
	};

	gpb: gpb {
		gpio-controller;
		#gpio-cells = <2>;
		interrupt-controller;
		#interrupt-cells = <2>;
		interrupts = <GIC_SPI INT_GPIO0 IRQ_TYPE_LEVEL_HIGH>;
		clk-src = <CLK_HFEOSC>;
		clk-src-div = <CLK_DIV_1>;
	};

	gpc: gpc {
		gpio-controller;
		#gpio-cells = <2>;
	};

	gpd: gpd {
		gpio-controller;
		#gpio-cells = <2>;
	};

	gpe: gpe {
		gpio-controller;
		#gpio-cells = <2>;
		interrupt-controller;
		#interrupt-cells = <2>;
		interrupts = <GIC_SPI INT_GPIO1 IRQ_TYPE_LEVEL_HIGH>;
		clk-src = <CLK_HFEOSC>;
		clk-src-div = <CLK_DIV_1>;
	};

	gpf: gpf {
		gpio-controller;
		#gpio-cells = <2>;
		interrupt-controller;
		#interrupt-cells = <2>;
		interrupts = <GIC_SPI INT_GPIO2 IRQ_TYPE_LEVEL_HIGH>;
		clk-src = <CLK_HFEOSC>;
		clk-src-div = <CLK_DIV_1>;
	};

	gpg: gpg {
		gpio-controller;
		#gpio-cells = <2>;
		interrupt-controller;
		#interrupt-cells = <2>;
		interrupts = <GIC_SPI INT_GPIO2 IRQ_TYPE_LEVEL_HIGH>;
		clk-src = <CLK_HFEOSC>;
		clk-src-div = <CLK_DIV_1>;
	};

	gpj: gpj {
		gpio-controller;
		#gpio-cells = <2>;
	};

	/*
	 * Pin groups
	 */

	pinctrl_uart0_0: uart0-grp0 {
		lombo,pins = "gpe-5", "gpe-6";
		lombo,pin-function = <7>;
		lombo,pin-pud = <DISABLE_PULL_UP_DOWN>;
		lombo,pin-drv = <DRV_LEVEL2>;
		lombo,pin-pud-reg = <PULL_100K>;
	};

	pinctrl_uart0_1: uart0-grp1 {
		lombo,pins = "gpa-4", "gpa-5";
		lombo,pin-function = <3>;
		lombo,pin-pud = <DISABLE_PULL_UP_DOWN>;
		lombo,pin-drv = <DRV_LEVEL2>;
		lombo,pin-pud-reg = <PULL_100K>;
	};

	pinctrl_uart0_2: uart0-grp2 {
		lombo,pins = "gpd-4", "gpd-5";
		lombo,pin-function = <4>;
		lombo,pin-pud = <DISABLE_PULL_UP_DOWN>;
		lombo,pin-drv = <DRV_LEVEL2>;
		lombo,pin-pud-reg = <PULL_100K>;
	};

	pinctrl_uart1_0: uart1-grp0 {
		lombo,pins = "gpe-4", "gpe-7";
		lombo,pin-function = <5>;
		lombo,pin-pud = <DISABLE_PULL_UP_DOWN>;
		lombo,pin-drv = <DRV_LEVEL2>;
		lombo,pin-pud-reg = <PULL_100K>;
	};

	pinctrl_uart1_1: uart1-grp1 {
		lombo,pins = "gpe-4", "gpe-7", "gpe-5", "gpe-6";
		lombo,pin-function = <5>;
		lombo,pin-pud = <DISABLE_PULL_UP_DOWN>;
		lombo,pin-drv = <DRV_LEVEL2>;
		lombo,pin-pud-reg = <PULL_100K>;
	};


	pinctrl_uart2: uart2-grp {
		lombo,pins = "gpe-0", "gpe-3";
		lombo,pin-function = <4>;
		lombo,pin-pud = <DISABLE_PULL_UP_DOWN>;
		lombo,pin-drv = <DRV_LEVEL2>;
		lombo,pin-pud-reg = <PULL_100K>;
	};

	pinctrl_uart3: uart3-grp {
		lombo,pins = "gpe-8", "gpe-9";
		lombo,pin-function = <4>;
		lombo,pin-pud = <DISABLE_PULL_UP_DOWN>;
		lombo,pin-drv = <DRV_LEVEL2>;
		lombo,pin-pud-reg = <PULL_100K>;
	};

	/* mosi(D0), miso(D1) */
	pinctrl_spi0_0: spi0-grp0 {
		lombo,pins = "gpa-8", "gpa-9";
		lombo,pin-function = <3>;
		lombo,pin-drv = <DRV_LEVEL2>;
		lombo,pin-pud = <DISABLE_PULL_UP_DOWN>;
	};

	pinctrl_spi0_0_1: spi0-grp0-1 {
		lombo,pins = "gpa-8", "gpa-9";
		lombo,pin-function = <3>;
		lombo,pin-drv = <DRV_LEVEL3>;
		lombo,pin-pud = <DISABLE_PULL_UP_DOWN>;
	};

	pinctrl_spi0_0_2: spi0-grp0-2 {
		lombo,pins = "gpa-8", "gpa-9";
		lombo,pin-function = <3>;
		lombo,pin-drv = <DRV_LEVEL4>;
		lombo,pin-pud = <DISABLE_PULL_UP_DOWN>;
	};

	/* clk */
	pinctrl_spi0_1: spi0-grp1 {
		lombo,pins = "gpa-7";
		lombo,pin-function = <3>;
		lombo,pin-drv = <DRV_LEVEL3>;
		lombo,pin-pud = <ENABLE_PULL_DOWN>;
		lombo,pin-pud-reg = <PULL_100K>;
	};

	pinctrl_spi0_1_1: spi0-grp1-1 {
		lombo,pins = "gpa-7";
		lombo,pin-function = <3>;
		lombo,pin-drv = <DRV_LEVEL4>;
		lombo,pin-pud = <ENABLE_PULL_DOWN>;
		lombo,pin-pud-reg = <PULL_100K>;
	};

	pinctrl_spi0_1_2: spi0-grp1-2 {
		lombo,pins = "gpa-7";
		lombo,pin-function = <3>;
		lombo,pin-drv = <DRV_LEVEL5>;
		lombo,pin-pud = <ENABLE_PULL_DOWN>;
		lombo,pin-pud-reg = <PULL_100K>;
	};

	/* cs0 */
	pinctrl_spi0_2: spi0-grp2 {
		lombo,pins = "gpa-6";
		lombo,pin-function = <3>;
		lombo,pin-drv = <DRV_LEVEL1>;
		lombo,pin-pud = <ENABLE_PULL_UP>;
		lombo,pin-pud-reg = <PULL_100K>;
	};

	pinctrl_spi0_2_1: spi0-grp2-1 {
		lombo,pins = "gpa-6";
		lombo,pin-function = <3>;
		lombo,pin-drv = <DRV_LEVEL2>;
		lombo,pin-pud = <ENABLE_PULL_UP>;
		lombo,pin-pud-reg = <PULL_100K>;
	};

	pinctrl_spi0_2_2: spi0-grp2-2 {
		lombo,pins = "gpa-6";
		lombo,pin-function = <3>;
		lombo,pin-drv = <DRV_LEVEL3>;
		lombo,pin-pud = <ENABLE_PULL_UP>;
		lombo,pin-pud-reg = <PULL_100K>;
	};

	/* wp(D2), hold(D3) */
	pinctrl_spi0_3: spi0-grp3 {
		lombo,pins = "gpa-10", "gpa-11";
		lombo,pin-function = <3>;
		lombo,pin-drv = <DRV_LEVEL2>;
		lombo,pin-pud = <ENABLE_PULL_UP>;
		lombo,pin-pud-reg = <PULL_20K>;
	};

	pinctrl_spi0_3_1: spi0-grp3-1 {
		lombo,pins = "gpa-10", "gpa-11";
		lombo,pin-function = <3>;
		lombo,pin-drv = <DRV_LEVEL3>;
		lombo,pin-pud = <ENABLE_PULL_UP>;
		lombo,pin-pud-reg = <PULL_20K>;
	};

	pinctrl_spi0_3_2: spi0-grp3-2 {
		lombo,pins = "gpa-10", "gpa-11";
		lombo,pin-function = <3>;
		lombo,pin-drv = <DRV_LEVEL4>;
		lombo,pin-pud = <ENABLE_PULL_UP>;
		lombo,pin-pud-reg = <PULL_20K>;
	};

	/* mosi(D0), miso(D1) */
	pinctrl_spi2_0: spi2-grp0 {
		lombo,pins = "gpe-6", "gpe-7";
		lombo,pin-function = <3>;
		lombo,pin-drv = <DRV_LEVEL2>;
		lombo,pin-pud = <DISABLE_PULL_UP_DOWN>;
	};

	/* clk */
	pinctrl_spi2_1: spi2-grp1 {
		lombo,pins = "gpe-5";
		lombo,pin-function = <3>;
		lombo,pin-drv = <DRV_LEVEL3>;
		lombo,pin-pud = <ENABLE_PULL_DOWN>;
		lombo,pin-pud-reg = <PULL_100K>;
	};

	/* cs0 */
	pinctrl_spi2_2: spi2-grp2 {
		lombo,pins = "gpe-4";
		lombo,pin-function = <3>;
		lombo,pin-drv = <DRV_LEVEL1>;
		lombo,pin-pud = <ENABLE_PULL_UP>;
		lombo,pin-pud-reg = <PULL_100K>;
	};

	pinctrl_i2s0: i2s0-grp {
		lombo,pins = "gpe-10", "gpe-11", "gpe-12", "gpe-13", "gpe-14";
		lombo,pin-function = <3>;
		lombo,pin-pud = <DISABLE_PULL_UP_DOWN>;
		lombo,pin-drv = <DRV_LEVEL3>;
		lombo,pin-pud-reg = <PULL_100K>;
	};

	pinctrl_sdc0_clk: sdc0_clk-grp {
		lombo,pins = "gpa-2";
		lombo,pin-function = <4>;
		lombo,pin-pud = <DISABLE_PULL_UP_DOWN>;
		lombo,pin-drv = <DRV_LEVEL3>;
		lombo,pin-pud-reg = <PULL_100K>;
	};

	pinctrl_sdc0_cmd_data_x4: sdc0_cmd_data_x4-grp {
		lombo,pins = "gpa-3", "gpa-1", "gpa-0", "gpa-5", "gpa-4";
		lombo,pin-function = <4>;
		lombo,pin-pud = <ENABLE_PULL_UP>;
		lombo,pin-drv = <DRV_LEVEL2>;
		lombo,pin-pud-reg = <PULL_20K>;
	};

	pinctrl_sdc0_data_h4: sdc0_data_h4-grp {
		lombo,pins = "gpa-6", "gpa-7", "gpa-8","gpa-9";
		lombo,pin-function = <4>;
		lombo,pin-pud = <ENABLE_PULL_UP>;
		lombo,pin-drv = <DRV_LEVEL2>;
		lombo,pin-pud-reg = <PULL_20K>;
	};

	pinctrl_sdc0_cmd_data_x1: sdc0_cmd_data_x1-grp {
		lombo,pins = "gpa-3", "gpa-1", "gpa-4";
