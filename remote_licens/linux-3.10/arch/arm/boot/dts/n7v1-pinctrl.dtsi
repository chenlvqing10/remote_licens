		lombo,pin-pud-reg = <PULL_20K>;
	};

	pinctrl_sdc2_clk: sdc2_clk-grp {
		lombo,pins = "gpa-12";
		lombo,pin-function = <5>;
		lombo,pin-pud = <DISABLE_PULL_UP_DOWN>;
		lombo,pin-drv = <DRV_LEVEL4>;
		lombo,pin-pud-reg = <PULL_100K>;
	};

	pinctrl_sdc2_cmd_data_x4: sdc2_cmd_data_x4-grp {
		lombo,pins = "gpa-13", "gpa-11", "gpa-10", "gpa-15", "gpa-14";
		lombo,pin-function = <5>;
		lombo,pin-pud = <ENABLE_PULL_UP>;
		lombo,pin-drv = <DRV_LEVEL3>;
		lombo,pin-pud-reg = <PULL_20K>;
	};

	pinctrl_sdc2_data_h4: sdc2_data_h4-grp {
