	};

	pinctrl_sdc0_clk_uhs: sdc0_clk_uhs-grp {
		lombo,pins = "gpa-2";
		lombo,pin-function = <4>;
		lombo,pin-pud = <DISABLE_PULL_UP_DOWN>;
		lombo,pin-drv = <DRV_LEVEL7>;
		lombo,pin-pud-reg = <PULL_100K>;
	};

	pinctrl_sdc0_cmd_data_x4_uhs: sdc0_cmd_data_x4_uhs-grp {
		lombo,pins = "gpa-3", "gpa-1", "gpa-0", "gpa-5", "gpa-4";
		lombo,pin-function = <4>;
		lombo,pin-pud = <ENABLE_PULL_UP>;
		lombo,pin-drv = <DRV_LEVEL6>;
		lombo,pin-pud-reg = <PULL_20K>;
	};

	pinctrl_sdc0_data_h4_uhs: sdc0_data_h4_uhs-grp {
		lombo,pins = "gpa-6", "gpa-7", "gpa-8","gpa-9";
		lombo,pin-function = <4>;
		lombo,pin-pud = <ENABLE_PULL_UP>;
		lombo,pin-drv = <DRV_LEVEL6>;
		lombo,pin-pud-reg = <PULL_20K>;
	};

	pinctrl_sdc1_clk: sdc1_clk-grp {
		lombo,pins = "gpd-2";
		lombo,pin-function = <3>;
		lombo,pin-pud = <DISABLE_PULL_UP_DOWN>;
		lombo,pin-drv = <DRV_LEVEL2>;
		lombo,pin-pud-reg = <PULL_100K>;
	};

	pinctrl_sdc1_cmd_data_x4: sdc1_cmd_data_x4-grp {
		lombo,pins = "gpd-3", "gpd-1", "gpd-0", "gpd-5", "gpd-4";
		lombo,pin-function = <3>;
		lombo,pin-pud = <ENABLE_PULL_UP>;
		lombo,pin-drv = <DRV_LEVEL2>;
		lombo,pin-pud-reg = <PULL_20K>;
	};

	pinctrl_sdc1_cmd_data_x1: sdc1_cmd_data_x1-grp {
		lombo,pins = "gpd-3", "gpd-1", "gpd-4";
		lombo,pin-function = <3>;
		lombo,pin-pud = <ENABLE_PULL_UP>;
		lombo,pin-drv = <DRV_LEVEL5>;
		lombo,pin-pud-reg = <PULL_20K>;
	};

	pinctrl_sdc1_clk_uhs: sdc1_clk_uhs-grp {
		lombo,pins = "gpd-2";
		lombo,pin-function = <3>;
		lombo,pin-pud = <DISABLE_PULL_UP_DOWN>;
		lombo,pin-drv = <DRV_LEVEL7>;
		lombo,pin-pud-reg = <PULL_100K>;
	};

	pinctrl_sdc1_cmd_data_x4_uhs: sdc1_cmd_data_x4_uhs-grp {
		lombo,pins = "gpd-3", "gpd-1", "gpd-0", "gpd-5", "gpd-4";
